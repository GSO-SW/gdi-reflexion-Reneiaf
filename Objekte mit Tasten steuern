Rectangel spieler;

private void FrmFrogger_KeyDown(object sender, KeyEventArgs e)
{
Ich kann dem Objekt eine Taste zuweisen die dafür zuständig ist das sie nach oben läuft 
    if (e.KeyCode == Keys.Up)
    {
        spieler.Y -= hoeheJeBereich;
    }

Ich kann dem Objekt eine Taste zuweisen die dafür zuständig ist das sie nach unten läuft 

    if (e.KeyCode == Keys.Down)
    {
        spieler.Y += hoeheJeBereich;
    }

Ich kann dem Objekt eine Taste zuweisen die dafür zuständig ist das sie nach links läuft 

    if (e.KeyCode == Keys.Left)
    { 
         spieler.X -= breite / anzahlBereiche;
    }

  Ich kann dem Objekt eine Taste zuweisen die dafür zuständig ist das sie nach rechts läuft 

    if (e.KeyCode == Keys.Right)
    {
            spieler.X += breite / anzahlBereiche;
    }

    this.Refresh(); // Aktualisierung des Bildschirms nach der Spielerbewegung
}
